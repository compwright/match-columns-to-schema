{
  "name": "match-columns-to-schema",
  "version": "1.3.0",
  "homepage": "https://github.com/compwright/match-columns-to-schema",
  "description": "Schema field matching algorithm for spreadsheet uploads",
  "keywords": [
    "rxjs",
    "schema",
    "csv",
    "auto-detect",
    "algorithm",
    "reactivex",
    "stream",
    "match"
  ],
  "source": "dist/node.js",
  "browser": "dist/browser.min.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/compwright/match-columns-to-schema.git"
  },
  "author": {
    "name": "Jonathon Hill",
    "email": "jonathon@compwright.com",
    "url": "https://compwright.com"
  },
  "license": "MIT",
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "pretest": "semistandard src/*.js src/**/*.js test/*.js test/**/*.js --fix",
    "test": "nyc mocha --recursive test/ --exit",
    "demo": "cat ./demo/list.csv | node demo",
    "build:node": "parcel build ./src/index.js --target node --out-file node.js --no-minify --no-source-maps",
    "build:browser": "parcel build ./src/index.js --target browser --out-file browser.min.js",
    "build": "npm run build:node && npm run build:browser",
    "prepublish": "npm run build && git commit -am \"Build\"",
    "publish": "git push origin --tags && npm run changelog && git push origin",
    "release:pre": "npm version prerelease && npm publish --tag pre --access public",
    "release:patch": "npm version patch && npm publish --access public",
    "release:minor": "npm version minor && npm publish --access public",
    "release:major": "npm version major && npm publish --access public",
    "changelog": "github_changelog_generator -u compwright -p match-columns-to-schema && git add CHANGELOG.md && git commit -am \"Updating changelog\""
  },
  "semistandard": {
    "env": [
      "node",
      "browser",
      "mocha"
    ]
  },
  "nyc": {
    "cache": true,
    "cacheDir": ".nyc_cache",
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "browserslist": "> 0.25%, not dead",
  "dependencies": {
    "d3-array": "^2.9.1",
    "papaparse": "^5.2.0",
    "rxjs": "^5.5.12",
    "string-similarity": "^4.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/preset-env": "^7.13.15",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "parcel-bundler": "^1.12.5",
    "semistandard": "*"
  }
}
